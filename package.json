{
  "name": "multizones-apple",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:store": "turbo run dev --filter=home --filter=store",
    "dev:iphone": "turbo run dev --filter=home --filter=iphone",
    "dev:mac": "turbo run dev --filter=home --filter=mac",
    "dev:clean": "npm run clean && turbo run dev",
    "clean": "rm -rf apps/*/.next apps/*/.turbo .turbo",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.4",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
